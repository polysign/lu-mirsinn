{
  "name": "lu-mirsinn",
  "private": true,
  "version": "0.0.12",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "postinstall": "npm-run-all install:*",
    "install:icons": "cp -r ./node_modules/@phosphor-icons/core/assets ./src/assets/icons",
    "postversion": "node scripts/sync-version.js && git add src/components/app-home/app-home.tsx"
  },
  "dependencies": {
    "@phosphor-icons/core": "^2.1.1",
    "firebase": "^10.14.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@stencil/core": "4.38.2",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-cli": "^30.2.0",
    "puppeteer": "^24.25.0",
    "stencil-router-v2": "^0.6.0",
    "workbox-build": "7.3.0"
  },
  "license": "MIT"
}
